#!/usr/bin/env perl
require 5.008;
BEGIN { $^W = 1; }
use strict;

# Move into the data directory where .pdf and .out live
chdir("destinations") or die "chdir testdir failed: $!\n";

require TestDriver;

my $td = new TestDriver('destinations');

$td->runtest("named destination helpers",
             {$td->COMMAND => "destinations dests.pdf"},
             {$td->FILE => "dests.out",
              $td->EXIT_STATUS => 0},
             $td->NORMALIZE_NEWLINES);

$td->report(1);
