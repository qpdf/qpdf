#!/usr/bin/env perl
require 5.008;
use warnings;
use strict;

require TestDriver;

my $td = new TestDriver('c++-11');

$td->runtest("C++-11",
	     {$td->COMMAND => "cxx11"},
	     {$td->STRING => "assertions passed\n",
	      $td->EXIT_STATUS => 0},
	     $td->NORMALIZE_NEWLINES);

$td->report(1);
