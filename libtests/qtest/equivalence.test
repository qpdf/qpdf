#!/usr/bin/env perl
require 5.008;
use warnings;
use strict;

chdir("equivalence") or die "chdir testdir failed: $!\n";

require TestDriver;

my $td = new TestDriver('equivalence');

$td->runtest("equivalence",
             {$td->COMMAND => "equivalence"},
             {$td->FILE => "equivalence.out", $td->EXIT_STATUS => 0},
             $td->NORMALIZE_NEWLINES);

$td->report(1);
